[alias]
   co = checkout
   ci = commit
   st = status
   br = branch
   sr = svn rebase
   sd = svn dcommit
   rhh = reset --hard HEAD
   amend = commit --amend -aC HEAD
   show-files = show --name-status
   pull = pull --log
   cont = rebase --continue
   l3 = log -3
   l5 = log -5
   l10 = log -10
   aa = add -A
   dag = log --graph  --date-order
   
[color]
   ui = auto
[color "status"]
   added = yellow
   changed = green
   untracked = cyan
[format]
   pretty = format:"%h %C(yellow)%ci%Creset %Cgreen%an%Creset | %s"
[push]
   default = tracking
[core]
	autocrlf = false
	excludesfile = ~/.gitignore
	pager = less -FMRiX
[receive]
	denyCurrentBranch = false
[user]
	name = Raghunandan Ramakrishna Rao
	email = r.raghunandan@gmail.com
[branch]
	autosetuprebase = always

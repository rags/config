[alias]
   co = checkout
   ci = commit
   st = status
   br = branch
   sr = svn rebase
   sd = svn dcommit
   rhh = reset --hard head
[color]
   ui = auto
[color "status"]
   added = yellow
   changed = green
   untracked = cyan
[format]
   pretty = format:"%h %C(yellow)%ci%Creset %Cgreen%an%Creset | %s"
[push]
   default = tracking
